{
    "name": "@uppercod/hash",
    "type": "module",
    "version": "1.1.0",
    "description": "Generates a hash based on [anteesor, current, successor]",
    "main": "./hash.js",
    "module": "./src/hash.js",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "test": "ava",
        "prepare": "npm run build && npm test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/UpperCod/hash.git"
    },
    "author": "UpperCod",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/UpperCod/hash/issues"
    },
    "homepage": "https://github.com/UpperCod/hash#readme",
    "dependencies": {},
    "devDependencies": {
        "ava": "^3.10.1",
        "rollup": "^2.21.0"
    },
    "typings": "types.d.ts",
    "ava": {
        "files": [
            "tests/**/*.test.js"
        ]
    },
    "exports": {
        ".": {
            "import": "./src/hash.js",
            "require": "./src/hash.js"
        }
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": false
    }
}
